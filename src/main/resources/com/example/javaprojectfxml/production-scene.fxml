<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaprojectfxml.ProductionSceneController">
   <children>
      <Button fx:id="allmovies" layoutX="142.0" layoutY="27.0" mnemonicParsing="false" onAction="#onAllMovieClick" prefHeight="26.0" prefWidth="142.0" text="Show All Movie" />
      <Button fx:id="recentmovies" layoutX="144.0" layoutY="81.0" mnemonicParsing="false" onAction="#onRecentMovies" prefHeight="26.0" prefWidth="173.0" text="Most Recenet Movies" />
      <Button fx:id="maximumrevenue" layoutX="144.0" layoutY="133.0" mnemonicParsing="false" onAction="#onMaximumRevenue" prefHeight="26.0" prefWidth="207.0" text="Movies with maximum revenue" />
      <Button fx:id="totalprofit" layoutX="143.0" layoutY="183.0" mnemonicParsing="false" onAction="#onTotalProfit" prefHeight="26.0" prefWidth="116.0" text="Total Profit" />
      <Button fx:id="back" layoutX="280.0" layoutY="342.0" mnemonicParsing="false" onAction="#onBackClick" text="Back" />
      <Button fx:id="addmovie" layoutX="144.0" layoutY="233.0" mnemonicParsing="false" onAction="#onAddMovieClick" text="Add Movie" />
      <Button fx:id="transfermovie" layoutX="144.0" layoutY="279.0" mnemonicParsing="false" onAction="#onTransferMovie" text="Transfer Movie" />
   </children>
</AnchorPane>
